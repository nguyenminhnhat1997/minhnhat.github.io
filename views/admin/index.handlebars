<a href="/admin/add" class="btn btn-danger">Add project</a>
<a href="/" class="btn btn-success pull-right">Frontend</a>
 <table class="table table-bordered">
    <thead>
      <tr>
      	<th></th>
        <th>Title</th>
        <th>Service</th>
        <th>URL</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
    	{{#if data}}
    		{{#each data}}
		      <tr>
		      	<td><img src="/uploads/{{image}}" alt="" class="small-img"></td>
		        <td>{{title}}</td>
		        <td>{{service}}</td>
		        <td>{{url}}</td>
		        <td>
		        	<a href="/admin/edit/{{_id}}" class="btn btn-success" >Edit</a>
					<a class="btn btn-danger delete-project" data-id="{{_id}}" > Delete</a>
		        </td>
		      </tr>
		    {{/each}}
	    {{else}}
	    	<p>No profile</p>
      	{{/if}}
    </tbody>
  </table>